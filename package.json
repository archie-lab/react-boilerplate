{
  "name": "archie-react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "precommit": "npm run eslint",
    "pretest": "npm run test:clean && npm run eslint",
    "build:clean": "rimraf ./dist",
    "clean": "npm run test:clean && npm run build:clean",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js --mode development --hot --max-old-space-size=4096",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js --mode production --progress --profile --colors",
    "eslint": "eslint --ignore-path .gitignore --color src/**/*.js",
    "eslint:fix": "eslint --ignore-path .gitignore --color --fix src",
    "test": "jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --watchAll"
  },
  "author": "Archie",
  "license": "ISC",
  "keywords": [
    "React",
    "redux",
    "redux-saga",
    "webpack",
    "pwa",
    "ES6"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
